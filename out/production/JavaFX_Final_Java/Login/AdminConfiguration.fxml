                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Label layoutX="3.0" layoutY="27.0" text="Admin Configuration">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="375.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@project Image/logo add student.png" />
         </image>
      </ImageView>
      <Label layoutX="183.0" layoutY="349.0" text="Name">
         <font>
            <Font name="Tahoma Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="178.0" layoutY="488.0" text="Password">
         <font>
            <Font name="Tahoma Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="181.0" layoutY="418.0" text="Email">
         <font>
            <Font name="Tahoma Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="submit" layoutX="150.0" layoutY="625.0" mnemonicParsing="false" onAction="#btnupdate" prefHeight="50.0" prefWidth="120.0" text="Update">
